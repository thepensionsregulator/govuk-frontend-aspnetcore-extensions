n  ]\\\\r\\\\n}\\\"\\r\\n    }\\r\\n  ],\\r\\n  \\\"settingsData\\\": [\\r\\n    {\\r\\n      \\\"contentTypeKey\\\": \\\"a248d9ad-85e5-446a-9c06-c6a72cd3f34f\\\",\\r\\n      \\\"udi\\\": \\\"umb://element/ae57e41a0a6845089b72e59c0af5c7b7\\\",\\r\\n      \\\"columnSize\\\": \\\"[\\\\\\\"two-thirds\\\\\\\"]\\\",\\r\\n      \\\"columnSizeFromDesktop\\\": null,\\r\\n      \\\"renderRowsAndColumnsForChildBlocks\\\": 0\\r\\n    }\\r\\n  ]\\r\\n}\"\r\n    },\r\n    {\r\n      \"contentTypeKey\": \"e0876897-88f4-4dcf-ae0a-ec5a37e3a185\",\r\n      \"udi\": \"umb://element/23e623f6d3114a39a532ebebb8008105\",\r\n      \"text\": \"<p>Unfortunately adding fieldset errors to the error summary is not yet supported.</p>\"\r\n    }\r\n  ],\r\n  \"settingsData\": [\r\n    {\r\n      \"contentTypeKey\": \"21721e40-fe18-447b-bbd6-00c02bd5539b\",\r\n      \"udi\": \"umb://element/27958e2ee8fe482689ef3e5268e17f38\",\r\n      \"columnSize\": null,\r\n      \"columnSizeFromDesktop\": null,\r\n      \"cssClassesForRow\": null\r\n    },\r\n    {\r\n      \"contentTypeKey\": \"7f2977e1-5298-4fee-9c8c-5abeeafc1d63\",\r\n      \"udi\": \"umb://element/39b4b79c1d854eff8c37204a0a19d453\",\r\n      \"columnSize\": null,\r\n      \"columnSizeFromDesktop\": null,\r\n      \"cssClassesForRow\": \"govuk-form-group govuk-form-group--error\"\r\n    },\r\n    {\r\n      \"contentTypeKey\": \"21721e40-fe18-447b-bbd6-00c02bd5539b\",\r\n      \"udi\": \"umb://element/e1f7adbdc36d4b61b58a766fab373143\",\r\n      \"columnSize\": null,\r\n      \"columnSizeFromDesktop\": null\r\n    },\r\n    {\r\n      \"contentTypeKey\": \"7f2977e1-5298-4fee-9c8c-5abeeafc1d63\",\r\n      \"udi\": \"umb://element/d3d389401f03409e8ad6b08b9e9cf44b\",\r\n      \"columnSize\": \"[\\\"full\\\"]\",\r\n      \"columnSizeFromDesktop\": null,\r\n      \"cssClassesForRow\": \"govuk-form-group govuk-form-group--error\"\r\n    },\r\n    {\r\n      \"contentTypeKey\": \"21721e40-fe18-447b-bbd6-00c02bd5539b\",\r\n      \"udi\": \"umb://el